<Window x:Class="Project.ItemEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="ItemEdit" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgItems" >
            <TextBlock Width="20" Text="{Binding ItemName}"></TextBlock>
            <TextBlock Text="asd" FontSize="20"></TextBlock>
            <TextBlock Text="asd"></TextBlock>
            <TextBlock Text="asd"></TextBlock>
        </DataGrid>
    </Grid>
</Window>
